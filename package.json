{
  "name": "@kazize/create-ts-starter-1",
  "version": "1.0.1",
  "description": "TypeScript Express.js backend boilerplate on AWS Lambda",
  "bin": "bin/createStarter.js",
  "files": [
    "bin/"
  ],
  "repository": "https://github.com/kazizehsan/ts-starter-1.git",
  "author": "Kazi Ehsan Aziz <kazizehsan@gmail.com>",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "compile": "tsoa spec-and-routes && tsc --build",
    "postcompile": "cp package*json dist/",
    "tsoa:watch": "onchange 'src/**/*.ts' -e 'src/routes/routes.ts' -e 'src/specs/*.json' -- tsoa spec-and-routes",
    "compile:watch": "onchange 'src/routes/routes.ts' -- tsc --build",
    "pre:dev": "cross-env NODE_ENV=development nodemon --watch dist dist/index.js",
    "dev": "concurrently --kill-others \"yarn tsoa:watch\" \"yarn compile:watch\" \"yarn pre:dev\"",
    "create-bucket": "aws s3 mb s3://$npm_package_config_s3BucketName --region $npm_package_config_region",
    "delete-bucket": "aws s3 rb s3://$npm_package_config_s3BucketName --region $npm_package_config_region",
    "delete-stack": "aws cloudformation delete-stack --stack-name $npm_package_config_cloudFormationStackName --region $npm_package_config_region",
    "sam:build": "sam build --template ./sam-template.yaml",
    "sam:deploy": "sam deploy --stack-name $npm_package_config_cloudFormationStackName --capabilities CAPABILITY_IAM --s3-bucket $npm_package_config_s3BucketName --s3-prefix $npm_package_name --region $npm_package_config_region --no-fail-on-empty-changeset",
    "package-deploy": "yarn compile && yarn sam:build && yarn sam:deploy",
    "setup": "yarn install && (aws s3api get-bucket-location --bucket $npm_package_config_s3BucketName --region $npm_package_config_region || yarn create-bucket) && yarn package-deploy",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest -i --colors --verbose --detectOpenHandles",
    "test:ts": "cross-env NODE_OPTIONS=--experimental-vm-modules jest -i --colors --verbose --detectOpenHandles -- 'ts$'",
    "test:js": "cross-env NODE_OPTIONS=--experimental-vm-modules jest -i --colors --verbose --detectOpenHandles -- 'js$'",
    "test:watch": "cross-env NODE_OPTIONS=--experimental-vm-modules jest -i --watchAll",
    "coverage": "cross-env NODE_OPTIONS=--experimental-vm-modules jest -i --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check **/*.ts",
    "prettier:fix": "prettier --write **/*.ts",
    "postinstall": "[ -d '.git' ] && husky || echo 'skipping postinstall script husky'",
    "commit": "git add -A && cz"
  },
  "keywords": [
    "create-ts-starter-1",
    "node",
    "typescript",
    "node.js",
    "backend",
    "boilerplate",
    "starter",
    "template",
    "express",
    "express.js",
    "aws",
    "lambda",
    "rest",
    "api",
    "mongodb",
    "mongoose",
    "jest",
    "passport",
    "joi",
    "eslint",
    "prettier"
  ],
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@faker-js/faker": "^8.4.1",
    "@jest/globals": "^27.5.1",
    "@ryansonshine/commitizen": "^4.2.8",
    "@ryansonshine/cz-conventional-changelog": "^3.3.4",
    "@types/aws-lambda": "^8.10.137",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "2.8.8",
    "@types/express": "^4.17.13",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jest": "^27.4.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.12",
    "@types/nodemailer": "^6.4.4",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.11",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/validator": "^13.12.0",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "concurrently": "^7.1.0",
    "eslint": "^8.4.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-security": "^1.4.0",
    "husky": "^7.0.4",
    "jest": "^27.4.4",
    "lint-staged": "^12.1.2",
    "node-mocks-http": "^1.11.0",
    "nodemon": "^2.0.20",
    "onchange": "^7.1.0",
    "prettier": "^2.5.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.3",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@codegenie/serverless-express": "^4.14.1",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-mongo-sanitize": "^2.1.0",
    "express-rate-limit": "^5.5.1",
    "helmet": "^4.6.0",
    "http-status": "^1.5.0",
    "inquirer": "^10.2.2",
    "joi": "^17.5.0",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.4",
    "mongoose": "^7.0.2",
    "morgan": "^1.10.0",
    "nodemailer": "^6.7.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "pm2": "^5.1.2",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.2.0",
    "tsoa": "^6.4.0",
    "validator": "^13.7.0",
    "winston": "^3.3.3",
    "xss-clean": "^0.1.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@ryansonshine/cz-conventional-changelog"
    },
    "s3BucketName": "ts-starter-1",
    "region": "ap-south-1",
    "cloudFormationStackName": "TSStarter1Stack"
  }
}
